
<%= form_tag search_path, :method => 'get', :url => { :action => "search" } do %>
	
	<%= text_field_tag :search, params[:search], :placeholder => "Enter a hobby"%>
	<%= text_field_tag :place, params[:place], :placeholder => "City, State or Zip"%>
	<%= submit_tag "Search", :title => nil, :id => "search_button" %>
	
<% end %>
